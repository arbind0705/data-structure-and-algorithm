#include<bits/stdc++.h>
using namespace std;

int main(){

int arr[8][3];
    
    int i  = 0 ,j = 0;
    
    while(i < 8){
        j = 0;
        while(j < 3){
            if(j == 0){
                arr[i][j] = i/4;
            }else if(j == 1){
                arr[i][j] = (i/2)%4;
            }else {
                arr[i][j] = i % 2;
            }
            j++;
            
        }
        i++;
    }
    i = 0;
    cout << "binary 0 to 7"<<endl;
    while(i < 8){
        j = 0;
        while(j<3){
            cout << "\t" <<arr[i][j] << " ";
            j++;
        }
        cout << endl;
        i++;
    }
    
    return 0;

}
